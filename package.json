{
  "name": "spatialconnect",
  "version": "0.12.1",
  "description": "Javascript Library for SpatialConnect",
  "main": "dist/spatialconnect.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rimraf dist && rimraf native",
    "build": "npm run build-es6 && npm run build-native",
    "build-es6": "webpack",
    "build-native": "webpack --config webpack.native.js",
    "test": "./node_modules/.bin/jest .test.js",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "docs": "documentation build src/index.js -f md -o API.md"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boundlessgeo/spatialconnect-js"
  },
  "author": "Boundless Spatial, Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/boundlessgeo/spatialconnect-js/issues"
  },
  "homepage": "https://github.com/boundlessgeo/spatialconnect-js",
  "dependencies": {
    "lodash": "4.17.4",
    "rx": "3.1.2",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.8.0",
    "babel-jest": "18.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.8.0",
    "babelify": "7.2.0",
    "commitizen": "1.0.4",
    "cz-conventional-changelog": "1.1.2",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-webpack": "0.7.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "require": "2.4.20",
    "rimraf": "2.5.0",
    "webpack": "1.12.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
